            <t>&lt;xref format="default" target="section-anchor"/&gt;: <xref format="default" target="section-anchor"/>
            </t>
            <t>&lt;xref format="default" target="section-anchor"&gt;TEXT-CONTENT&lt;/xref&gt;: <xref format="default" target="section-anchor">TEXT-CONTENT</xref>
            </t>
         </section>
